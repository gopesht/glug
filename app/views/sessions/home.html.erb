




 <!-- Carousel
    ================================================== -->
    <div id="myCarousel" class="carousel slide">
  <ol class="carousel-indicators" id="carousel_btn">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <!-- Carousel items -->
  <div class="carousel-inner" id="carousel_main">
    <div class="active item"><img src="http://lukemcreynolds.com/files/wallpaper/GNU.jpg"></div>
    <div class="item"><img src="http://lukemcreynolds.com/files/wallpaper/GNU.jpg" ></div>
    <div class="item"><img src="http://lukemcreynolds.com/files/wallpaper/GNU.jpg" ></div>
  </div>
  <!-- Carousel nav -->
  <a class="carousel-control left" href="#myCarousel" data-slide="next">&lsaquo;</a>
  <a class="carousel-control right" href="#myCarousel" data-slide="prev">&rsaquo;</a>
</div>
    <!-- /.carousel -->





 <div class="ticker_cont">
 <div class="main_hd">
    <h4 class="ticker_hd">Welcome to NIT DGP LUG</h4>
  </div>
  <div id="ticker">
  <div id="ticker_hding"><h4 class="ticker_hd">Upcoming Events</h4></div>
    <table class="table table-hover table-responsive " rules="rows" id="table">

  <% Notification.all(:order =>'created_at DESC').each do |n| %>
    
      <tr>
        <th>
          <%= n.content %>
          <h4 class="ticker_tym_stamp"><%= time_ago_in_words(n.created_at)%> ago</h4>
        </th> 
      </tr>       

     <% end %> 
      
    </table>
  </div>
 </div>
 
    



    
    <script type="text/javascript">
      
      
      $('.carousel').carousel({
  interval: 2000
});
    </script>